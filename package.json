{
  "name": "ngneat",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "nx": "nx",
    "start": "ng serve --o --port=3000",
    "build": "ng build",
    "test": "nx test",
    "e2e": "nx e2e --watch",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "contributors:add": "all-contributors add",
    "hooks:pre-commit": "node hooks/pre-commit.js",
    "commit": "git-cz",
    "release:first": "npm run release -- --first-release",
    "build:lib": "ng build --project=dirty-check-forms",
    "test:lib": "nx test --project=dirty-check-forms",
    "dirty-check:release": "npm run dirty-check:version && npm run build:lib",
    "dirty-check:version": "standard-version --bumpFiles=libs/dirty-check-forms/package.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.0.0",
    "@angular/common": "9.0.0",
    "@angular/compiler": "9.0.0",
    "@angular/core": "9.0.0",
    "@angular/forms": "9.0.0",
    "@angular/platform-browser": "9.0.0",
    "@angular/platform-browser-dynamic": "9.0.0",
    "@angular/router": "9.0.0",
    "@ngneat/lib": "1.0.0",
    "@nrwl/angular": "9.1.2",
    "core-js": "^2.5.4",
    "ng-zorro-antd": "^8.5.2",
    "rxjs": "~6.5.0",
    "tslib": "^1.10.0",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-ng-packagr": "~0.900.7",
    "@angular/cli": "9.0.1",
    "@nrwl/workspace": "9.1.2",
    "@types/node": "~8.9.4",
    "dotenv": "6.2.0",
    "ng-packagr": "^9.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.0.0",
    "eslint": "6.8.0",
    "typescript": "~3.7.4",
    "prettier": "1.19.1",
    "@angular/compiler-cli": "9.0.0",
    "@angular/language-service": "9.0.0",
    "@angular-devkit/build-angular": "0.900.1",
    "codelyzer": "~5.0.1",
    "jest-preset-angular": "8.0.0",
    "@nrwl/jest": "9.1.2",
    "jest": "24.9.0",
    "@types/jest": "24.9.1",
    "ts-jest": "24.0.0",
    "cypress": "^4.1.0",
    "@nrwl/cypress": "9.1.2",
    "@commitlint/cli": "8.1.0",
    "@commitlint/config-angular": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "git-cz": "^3.2.1",
    "all-contributors-cli": "^6.8.1",
    "lint-staged": "^9.2.0",
    "standard-version": "^6.0.1",
    "husky": "^3.0.1",
    "cross-env": "^5.2.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $GIT_PARAMS",
      "pre-commit": "npm run hooks:pre-commit && lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,ts,html,component.html}": [
      "prettier --write",
      "git add"
    ]
  }
}
